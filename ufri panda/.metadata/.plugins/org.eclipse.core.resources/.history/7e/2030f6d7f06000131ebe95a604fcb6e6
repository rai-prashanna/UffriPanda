package trail;


import javax.swing.*;

public class Frame {
 
        public Frame(){
                JFrame frame = new JFrame();
                board b= new board();
                frame.add(frame,b);
                frame.setTitle("Ufri");
                frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
                frame.setSize(480,650);
                frame.setVisible(true);
                frame.setLocationRelativeTo(null);
        }
        public static void main(String[] args){
                new Frame();
        }
}